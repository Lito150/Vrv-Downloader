			 xmlns:local="clr-namespace:VrvDownloader"
			 xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options" ShutdownMode="OnExplicitShutdown"
			 >
	<Application.Resources>
		 <BitmapImage x:Key="BackgroundImage" po:Freeze="True" UriSource="/Images/k-on.jpg"/>
